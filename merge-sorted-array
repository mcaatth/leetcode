#include <iostream>
#include <vector>

class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        // if ();//TODO: what if m,n incorrect
        for (int i = 0; i < n; i++) {
            nums1[m+i] = nums2[i];
        }

        std::sort(nums1.begin(), nums1.end());
    }
};
